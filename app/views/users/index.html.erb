<div class="container">



<p id="notice"><%= notice %></p>

<h1>Users</h1>


<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Age</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |use| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.address %></td>
        <td><%= user.age %></td>
        <td><%= link_to "Show", user_path(user) %></td>
        <td><%= link_to "Edit",edit_user_path(user) %></td>
        <td><%= link_to "Destroy", user_path(user), method: :delete, date: {confilm: "Are you sure?"} %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<br>
<%= link_to "New User", new_user_path %>

</div>